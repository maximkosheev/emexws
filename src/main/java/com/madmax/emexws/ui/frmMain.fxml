                                 </columns>
                              </TableView>
                              <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="35.0" prefWidth="200.0" spacing="15.0">
                                 <children>
                                    <Button fx:id="btnClearDisposalItems" mnemonicParsing="false" text="Отчистить" />
                                    <Button fx:id="btnDisposalAll" mnemonicParsing="false" onAction="#onDisposalCode" text="Выдать" />
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </VBox.margin>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
